<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>JObfuscator Pro - Roblox Luau Edition</title>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.65.14/codemirror.min.css" rel="stylesheet">
    <link href="https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.65.14/theme/material-darker.min.css" rel="stylesheet">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.65.14/codemirror.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/codemirror/5.65.14/mode/lua/lua.min.js"></script>
    <style>
        * { margin: 0; padding: 0; box-sizing: border-box; }
        body { 
            font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
            background-color: #1e1e2e;
            color: #ffffff;
            min-height: 100vh;
            padding: 20px;
        }
        
        .container { 
            max-width: 1400px;
            margin: 0 auto;
            background: #2e2e3e;
            padding: 30px;
            border-radius: 20px;
            box-shadow: 0 20px 60px rgba(0, 0, 0, 0.5);
        }
        
        .header {
            text-align: center;
            margin-bottom: 30px;
        }
        
        .header h1 {
            font-size: 2.5em;
            color: #6200ea;
            margin-bottom: 10px;
        }
        
        .header p {
            color: #a0a0b0;
            font-size: 1.1em;
        }
        
        .badge {
            display: inline-block;
            background: #6200ea;
            padding: 5px 15px;
            border-radius: 20px;
            font-size: 0.8em;
            margin-top: 10px;
        }
        
        .settings-panel {
            background: #1e1e2e;
            padding: 20px;
            border-radius: 15px;
            margin-bottom: 20px;
            border: 1px solid #3e3e4e;
        }
        
        .settings-grid {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));
            gap: 15px;
            margin-top: 15px;
        }
        
        .setting-item {
            display: flex;
            align-items: center;
            justify-content: space-between;
            padding: 12px;
            background: #2e2e3e;
            border-radius: 8px;
            transition: all 0.3s;
        }
        
        .setting-item:hover {
            background: #3e3e4e;
            transform: translateX(5px);
        }
        
        .setting-item label {
            color: #e0e0f0;
            cursor: pointer;
            user-select: none;
        }
        
        .toggle {
            position: relative;
            width: 50px;
            height: 26px;
            background: #3e3e4e;
            border-radius: 13px;
            cursor: pointer;
            transition: background 0.3s;
        }
        
        .toggle.active { 
            background: #6200ea;
        }
        
        .toggle::after {
            content: '';
            position: absolute;
            top: 3px;
            left: 3px;
            width: 20px;
            height: 20px;
            background: white;
            border-radius: 50%;
            transition: transform 0.3s;
        }
        
        .toggle.active::after { transform: translateX(24px); }
        
        .slider-container {
            margin-top: 15px;
        }
        
        .slider-label {
            color: #e0e0f0;
            font-size: 0.9em;
            margin-bottom: 8px;
            display: flex;
            justify-content: space-between;
        }
        
        .slider {
            width: 100%;
            height: 6px;
            background: #3e3e4e;
            border-radius: 3px;
            outline: none;
            -webkit-appearance: none;
        }
        
        .slider::-webkit-slider-thumb {
            -webkit-appearance: none;
            appearance: none;
            width: 18px;
            height: 18px;
            background: #6200ea;
            cursor: pointer;
            border-radius: 50%;
        }
        
        .editor-container {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 20px;
            margin-bottom: 20px;
        }
        
        .editor-wrapper {
            background: #1e1e2e;
            border-radius: 15px;
            overflow: hidden;
            border: 1px solid #3e3e4e;
        }
        
        .editor-header {
            background: #6200ea;
            padding: 12px 20px;
            font-weight: bold;
            display: flex;
            justify-content: space-between;
            align-items: center;
        }
        
        .editor-header .copy-btn {
            background: rgba(255, 255, 255, 0.2);
            border: none;
            color: white;
            padding: 6px 16px;
            border-radius: 5px;
            cursor: pointer;
            font-size: 12px;
            transition: all 0.3s;
        }
        
        .editor-header .copy-btn:hover {
            background: rgba(255, 255, 255, 0.3);
        }
        
        .CodeMirror {
            height: 500px;
            font-size: 14px;
            font-family: 'Consolas', 'Monaco', 'Courier New', monospace;
        }
        
        .controls {
            display: flex;
            justify-content: center;
            gap: 15px;
            flex-wrap: wrap;
            margin-bottom: 20px;
        }
        
        button {
            background: #6200ea;
            color: white;
            border: none;
            padding: 15px 30px;
            border-radius: 10px;
            cursor: pointer;
            font-size: 16px;
            font-weight: bold;
            transition: all 0.3s;
        }
        
        button:hover {
            transform: translateY(-3px);
            box-shadow: 0 6px 20px rgba(98, 0, 234, 0.4);
        }
        
        button:active {
            transform: translateY(-1px);
        }
        
        .stats {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
            gap: 15px;
        }
        
        .stat-card {
            background: #1e1e2e;
            padding: 20px;
            border-radius: 12px;
            text-align: center;
            border: 1px solid #3e3e4e;
            transition: all 0.3s;
        }
        
        .stat-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 10px 25px rgba(98, 0, 234, 0.3);
        }
        
        .stat-card .label {
            color: #a0a0b0;
            font-size: 0.9em;
            margin-bottom: 8px;
        }
        
        .stat-card .value {
            color: #6200ea;
            font-size: 2em;
            font-weight: bold;
        }
        
        .notification {
            position: fixed;
            top: 20px;
            right: 20px;
            background: #4caf50;
            color: white;
            padding: 15px 25px;
            border-radius: 10px;
            box-shadow: 0 5px 15px rgba(0, 0, 0, 0.5);
            transform: translateX(400px);
            transition: transform 0.3s;
            z-index: 1000;
        }
        
        .notification.show {
            transform: translateX(0);
        }
        
        .warning-box {
            background: rgba(255,193,7,0.1);
            border: 2px solid rgba(255,193,7,0.5);
            border-radius: 10px;
            padding: 15px;
            margin-bottom: 20px;
            color: #ffd54f;
        }
        
        @media (max-width: 768px) {
            .editor-container { grid-template-columns: 1fr; }
            .settings-grid { grid-template-columns: 1fr; }
            .header h1 { font-size: 2em; }
        }
    </style>
</head>
<body>
<div class="container">
    <div class="header">
        <h1>JObfuscator</h1>
        <p>Advanced Roblox Luau Obfuscation Engine</p>
        <div class="badge">Version 4.0</div>
    </div>
    
    <div class="warning-box">
        <strong>⚠️ Roblox Notice:</strong> This version of the Obfuscator is really new. There may be bugs.
    </div>
    
    <div class="settings-panel">
        <h3 style="color: #e0e0f0; margin-bottom: 15px;">Obfuscation Settings</h3>
        <div class="settings-grid">
            <div class="setting-item">
                <label>Variable Mangling</label>
                <div class="toggle active" onclick="toggleSetting(this, 'varObf')"></div>
            </div>
            <div class="setting-item">
                <label>String Encryption</label>
                <div class="toggle active" onclick="toggleSetting(this, 'strObf')"></div>
            </div>
            <div class="setting-item">
                <label>Control Flow</label>
                <div class="toggle active" onclick="toggleSetting(this, 'ctrlFlow')"></div>
            </div>
            <div class="setting-item">
                <label>Dead Code</label>
                <div class="toggle active" onclick="toggleSetting(this, 'deadCode')"></div>
            </div>
            <div class="setting-item">
                <label>Number Encoding</label>
                <div class="toggle active" onclick="toggleSetting(this, 'numObf')"></div>
            </div>
            <div class="setting-item">
                <label>Constant Folding</label>
                <div class="toggle active" onclick="toggleSetting(this, 'constFold')"></div>
            </div>
            <div class="setting-item">
                <label>Table Lookup</label>
                <div class="toggle active" onclick="toggleSetting(this, 'tableLookup')"></div>
            </div>
            <div class="setting-item">
                <label>Proxy Functions</label>
                <div class="toggle active" onclick="toggleSetting(this, 'proxyFunc')"></div>
            </div>
        </div>
        <div class="slider-container">
            <div class="slider-label">
                <span>Obfuscation Intensity</span>
                <span id="intensityValue">5</span>
            </div>
            <input type="range" min="1" max="10" value="5" class="slider" id="intensity" oninput="updateIntensity(this.value)">
        </div>
    </div>
    
    <div class="editor-container">
        <div class="editor-wrapper">
            <div class="editor-header">
                <span>Input Code (Luau)</span>
            </div>
            <textarea id="inputEditor"></textarea>
        </div>
        <div class="editor-wrapper">
            <div class="editor-header">
                <span>Obfuscated Output</span>
                <button class="copy-btn" onclick="copyOutput()">Copy</button>
            </div>
            <textarea id="outputEditor"></textarea>
        </div>
    </div>
    
    <div class="controls">
        <button onclick="obfuscateCode()">OBFUSCATE</button>
        <button onclick="downloadCode()">DOWNLOAD</button>
        <button onclick="clearEditors()">CLEAR</button>
    </div>
    
    <div class="stats">
        <div class="stat-card">
            <div class="label">Original Size</div>
            <div class="value" id="originalSize">0</div>
        </div>
        <div class="stat-card">
            <div class="label">Obfuscated Size</div>
            <div class="value" id="obfuscatedSize">0</div>
        </div>
        <div class="stat-card">
            <div class="label">Size Increase</div>
            <div class="value" id="sizeIncrease">0%</div>
        </div>
        <div class="stat-card">
            <div class="label">Protection Level</div>
            <div class="value" id="protectionLevel">0%</div>
        </div>
        <div class="stat-card">
            <div class="label">Technique Layers</div>
            <div class="value" id="layerCount">0</div>
        </div>
    </div>
</div>

<div class="notification" id="notification"></div>

<script>
const inputEditor = CodeMirror.fromTextArea(document.getElementById("inputEditor"), {
    mode: "lua",
    theme: "material-darker",
    lineNumbers: true,
    lineWrapping: true,
    autoCloseBrackets: true,
    matchBrackets: true
});

const outputEditor = CodeMirror.fromTextArea(document.getElementById("outputEditor"), {
    mode: "lua",
    theme: "material-darker",
    lineNumbers: true,
    lineWrapping: true,
    readOnly: true
});

const settings = {
    varObf: true,
    strObf: true,
    ctrlFlow: true,
    deadCode: true,
    numObf: true,
    constFold: true,
    tableLookup: true,
    proxyFunc: true
};

let intensity = 5;

function toggleSetting(elem, name) {
    settings[name] = !settings[name];
    elem.classList.toggle('active');
}

function updateIntensity(val) {
    intensity = parseInt(val);
    document.getElementById('intensityValue').textContent = val;
}

function rand(len = 8) {
    const chars = 'abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ';
    let result = '_';
    for (let i = 0; i < len; i++) {
        result += chars[Math.floor(Math.random() * chars.length)];
    }
    return result + '_' + Math.random().toString(36).substring(2, 5);
}

function obfuscateVariables(code) {
    if (!settings.varObf) return code;
    const varMap = {};
    
    let obf = code.replace(/local\s+([a-zA-Z_]\w*)(\s*=|\s*,|\s*\n|\s*$)/g, (match, name, after) => {
        if (!varMap[name]) {
            varMap[name] = rand(intensity + 2);
        }
        return `local ${varMap[name]}${after}`;
    });
    
    obf = obf.replace(/function\s+([a-zA-Z_]\w*)/g, (match, name) => {
        if (!varMap[name]) {
            varMap[name] = rand(intensity + 3);
        }
        return `function ${varMap[name]}`;
    });
    
    for (let orig in varMap) {
        const regex = new RegExp(`\\b${orig}\\b`, 'g');
        obf = obf.replace(regex, varMap[orig]);
    }
    
    return obf;
}

function obfuscateStrings(code) {
    if (!settings.strObf) return code;
    
    return code.replace(/(["'])((?:\\.|(?!\1).)*?)\1/g, (match, quote, str) => {
        if (str.length === 0) return '""';
        if (str.length > 100) return match;
        
        const parts = [];
        for (let i = 0; i < str.length; i++) {
            const charCode = str.charCodeAt(i);
            
            if (intensity >= 7) {
                const chunks = [];
                let remaining = charCode;
                while (remaining > 0) {
                    const chunk = Math.min(remaining, Math.floor(Math.random() * 25) + 15);
                    chunks.push(chunk);
                    remaining -= chunk;
                }
                parts.push(`string.char(${chunks.join('+')})`);
            } else if (intensity >= 4) {
                const base = Math.floor(charCode / 2);
                const remainder = charCode % 2;
                parts.push(`string.char(${base}*2${remainder ? '+1' : ''})`);
            } else {
                parts.push(`string.char(${charCode})`);
            }
        }
        
        return parts.join('..');
    });
}

function obfuscateNumbers(code) {
    if (!settings.numObf) return code;
    
    return code.replace(/\b(\d+)\b/g, (match, num) => {
        const n = parseInt(num);
        if (n === 0) return '(1-1)';
        if (n === 1) return '(2-1)';
        
        const methods = [
            () => `(${n + Math.floor(Math.random() * 50) + 10}-${Math.floor(Math.random() * 50) + 10})`,
            () => `(${Math.floor(n / 2)}*2${n % 2 ? '+1' : ''})`,
            () => `(${n * 2}/2)`,
            () => `(${n}+0)`
        ];
        
        return methods[Math.floor(Math.random() * methods.length)]();
    });
}

function addConstantFolding(code) {
    if (!settings.constFold) return code;
    
    code = code.replace(/\btrue\b/g, '(1==1)');
    code = code.replace(/\bfalse\b/g, '(1==0)');
    
    return code;
}

function injectDeadCode(code) {
    if (!settings.deadCode) return code;
    
    const junkPatterns = [
        () => `local ${rand()}=${Math.random()*1000|0}`,
        () => `local ${rand()}=function()return ${Math.random()*100|0} end`,
        () => `local ${rand()}={${Array(intensity).fill(0).map(()=>Math.random()*100|0).join(',')}}`,
        () => `if false then local ${rand()}=0 end`,
        () => `for ${rand()}=1,0 do end`
    ];
    
    const lines = code.split('\n');
    const result = [];
    
    lines.forEach((line, i) => {
        if (Math.random() > (1 - intensity * 0.08) && i > 0 && line.trim()) {
            const numJunk = Math.floor(Math.random() * (intensity / 2)) + 1;
            for (let j = 0; j < numJunk; j++) {
                result.push(junkPatterns[Math.floor(Math.random() * junkPatterns.length)]());
            }
        }
        result.push(line);
    });
    
    return result.join('\n');
}

function addControlFlow(code) {
    if (!settings.ctrlFlow) return code;
    
    const stateVar = rand();
    const loopVar = rand();
    
    const wrapper = `local ${stateVar}=1 while ${stateVar}>0 do ${stateVar}=0 local ${loopVar}=0 repeat ${loopVar}=${loopVar}+1 if ${loopVar}>1 then break end ${code} until true end`;
    
    return wrapper;
}

function addTableLookup(code) {
    if (!settings.tableLookup) return code;
    
    const tableVar = rand();
    const lookupTable = `local ${tableVar}={${Array(20).fill(0).map((_,i)=>i).join(',')}}`;
    
    return lookupTable + '\n' + code;
}

function addProxyFunctions(code) {
    if (!settings.proxyFunc) return code;
    
    const proxies = [];
    const proxyMap = {};
    
    const commonFuncs = ['print', 'warn', 'wait', 'spawn'];
    commonFuncs.forEach(func => {
        const proxyName = rand();
        proxyMap[func] = proxyName;
        proxies.push(`local ${proxyName}=${func}`);
    });
    
    let obf = code;
    for (let orig in proxyMap) {
        const regex = new RegExp(`\\b${orig}\\(`, 'g');
        obf = obf.replace(regex, `${proxyMap[orig]}(`);
    }
    
    return proxies.join('\n') + '\n' + obf;
}

function obfuscateCode() {
    const code = inputEditor.getValue().trim();
    if (!code) {
        showNotification('Please enter some code first!', 'error');
        return;
    }
    
    let obf = code;
    let layerCount = 0;
    
    try {
        if (settings.varObf) { obf = obfuscateVariables(obf); layerCount++; }
        if (settings.proxyFunc) { obf = addProxyFunctions(obf); layerCount++; }
        if (settings.tableLookup) { obf = addTableLookup(obf); layerCount++; }
        if (settings.constFold) { obf = addConstantFolding(obf); layerCount++; }
        if (settings.numObf) { obf = obfuscateNumbers(obf); layerCount++; }
        if (settings.strObf) { obf = obfuscateStrings(obf); layerCount++; }
        if (settings.deadCode) { obf = injectDeadCode(obf); layerCount++; }
        if (settings.ctrlFlow) { obf = addControlFlow(obf); layerCount++; }
        
        const header = `-- Obfuscated by JObfuscator V4.0\n-- Protection Level: ${intensity * 10}%\n-- Layers: ${layerCount}\n-- Timestamp: ${new Date().toISOString()}\n\n`;
        
        const final = header + obf;
        
        outputEditor.setValue(final);
        updateStats(code, final, layerCount);
        showNotification('Code obfuscated successfully!', 'success');
    } catch (error) {
        showNotification('Error: ' + error.message, 'error');
    }
}

function updateStats(original, obfuscated, layers) {
    const origSize = original.length;
    const obfSize = obfuscated.length;
    const increase = ((obfSize - origSize) / origSize * 100).toFixed(1);
    
    let protection = 0;
    if (settings.varObf) protection += 15;
    if (settings.strObf) protection += 20;
    if (settings.ctrlFlow) protection += 15;
    if (settings.deadCode) protection += 10;
    if (settings.numObf) protection += 10;
    if (settings.constFold) protection += 10;
    if (settings.tableLookup) protection += 10;
    if (settings.proxyFunc) protection += 10;
    
    protection = Math.min(protection, 95);
    
    document.getElementById('originalSize').textContent = origSize;
    document.getElementById('obfuscatedSize').textContent = obfSize;
    document.getElementById('sizeIncrease').textContent = increase + '%';
    document.getElementById('protectionLevel').textContent = protection + '%';
    document.getElementById('layerCount').textContent = layers;
}

function copyOutput() {
    const code = outputEditor.getValue();
    if (!code) {
        showNotification('Nothing to copy!', 'error');
        return;
    }
    navigator.clipboard.writeText(code).then(() => {
        showNotification('Copied to clipboard!', 'success');
    });
}

function downloadCode() {
    const code = outputEditor.getValue();
    if (!code) {
        showNotification('Nothing to download!', 'error');
        return;
    }
    const blob = new Blob([code], { type: 'text/plain' });
    const link = document.createElement('a');
    link.href = URL.createObjectURL(blob);
    link.download = 'obfuscated_' + Date.now() + '.lua';
    link.click();
    showNotification('File downloaded!', 'success');
}

function clearEditors() {
    inputEditor.setValue('');
    outputEditor.setValue('');
    updateStats('', '', 0);
    showNotification('Editors cleared!', 'success');
}

function showNotification(message, type) {
    const notif = document.getElementById('notification');
    notif.textContent = message;
    notif.style.background = type === 'success' ? '#4caf50' : '#f44336';
    notif.classList.add('show');
    setTimeout(() => notif.classList.remove('show'), 3000);
}

inputEditor.setValue(`-- Roblox Script Example
local player = game.Players.LocalPlayer
local message = "Hello, Roblox!"

function greet(name)
    return "Hello, " .. name
end

for i = 1, 10 do
    wait(1)
    print(message)
end`);
</script>
</body>
</html>
